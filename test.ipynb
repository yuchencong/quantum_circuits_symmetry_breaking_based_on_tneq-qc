   "source": [
    "import torch\n",
    "\n",
    "# qubit 数\n",
    "D = 2\n",
    "d = 2 ** D\n",
    "\n",
    "# 随机 unitary（QR 分解法）\n",
    "X = torch.randn(d, d, dtype=torch.cfloat)\n",
    "Q, R = torch.linalg.qr(X)\n",
    "U = Q  # U 是 unitary\n",
    "\n",
    "# Frobenius norm squared\n",
    "fro_sq = torch.norm(U, p='fro')\n",
    "\n",
    "print(\"dimension d =\", d)\n",
    "print(\"Frobenius norm squared =\", fro_sq.item())"
   ]
  }
 ],
